import java.util.*;
class Linearsearch
{
public static void main(String args[])
{
Scanner sc=new Scanner(System.in);
int n=sc.nextInt();
int arr[]=new int[n]; 
for(int i=0;i<n;i++)
{
arr[i]=sc.nextInt();
}
System.out.print("Enter element to search:");
int x=sc.nextInt();
int flag=0;
for(int i=0;i<n;i++)
{
if(x==arr[i])
flag=flag+1;
}
if(flag==1)
System.out.print("Element is found");
else
System.out.print("Element is not found");
}
}
