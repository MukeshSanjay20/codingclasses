import java.util.*;
class revstack
{
public static void main(String args[])
{
Scanner sc=new Scanner(System.in);
String str=sc.next();
char ch[] = str.toCharArray();
Stack<Character> st = new Stack<Character>();
for(int i=0;i<ch.length;i++) 
{
st.push(ch[i]);
}
for(int i=0;i<ch.length;i++) 
{
ch[i]=st.pop();
System.out.print(ch[i]);
}
}
}
