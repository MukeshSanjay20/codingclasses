import java.util.*;
class patterns2
{
public static void main(String args[])
{
Scanner sc=new Scanner(System.in);
System.out.print("N=");
int N=sc.nextInt();
int i,j,k;
for(i=1;i<=N;i++)
{
for(j=1;j<=i-1;j++)
{
System.out.print("*");
}
System.out.println();
}
for(i=1;i<=N;i++)
{
for(k=i;k<=N;k++)
{
System.out.print("*");
}
System.out.println();
}
}
}
