import java.util.*;
class trailingzeroes
{
public static void main(String[] args) 
{
Scanner sc=new Scanner(System.in);
int n = sc.nextInt();
long fact = 1;
if(n>0)
{
for(int i = 1; i <= n; i++)
{
fact = fact * i;
}
System.out.println(fact);
int count=0;
while(fact%10==0)
{
count=count+1;
fact=fact/10;
}
System.out.println(count);
}
else 
System.out.println("0");
}
}
